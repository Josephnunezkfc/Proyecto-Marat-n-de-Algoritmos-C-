#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingrese el número de términos de Fibonacci: ";
    cin >> n;

    if (n <= 0) {
        cout << "Número inválido." << endl;
        return 1;
    }

    int a = 0, b = 1;

    for (int i = 1; i <= n; ++i) {
        cout << a << " ";
        int next = a + b;
        a = b;
        b = next;
    }
    cout << endl;

    return 0;
}
