#include <iostream>
#include <vector>


int main() {
std::vector<int> numeros = {10, 20, 30, 40, 50};


if (numeros.empty()) return 1;


int ultimo = numeros.back(); // Guardar el último elemento


// Desplazar elementos a la derecha
for (int i = numeros.size() - 1; i > 0; --i) {
numeros[i] = numeros[i - 1];
}


numeros[0] = ultimo; // Poner el último en el primero


std::cout << "Array rotado a la derecha: ";
for(int x : numeros) std::cout << x << " ";
std::cout << std::endl;


return 0;
}
