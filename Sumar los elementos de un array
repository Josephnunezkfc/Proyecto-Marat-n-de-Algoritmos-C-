

#include <iostream>


int main() {
const int TAM = 5;
int numeros[TAM];
int suma = 0;


std::cout << "Ingresa " << TAM << " números enteros para sumar:" << std::endl;
for (int i = 0; i < TAM; ++i) {
std::cout << "Número " << i + 1 << ": ";
std::cin >> numeros[i];
}


for (int i = 0; i < TAM; ++i) {
suma += numeros[i];
}


std::cout << "La suma de los elementos del array es: " << suma << std::endl;


return 0;
}
