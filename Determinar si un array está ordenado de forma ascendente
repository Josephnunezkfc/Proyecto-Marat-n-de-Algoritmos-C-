#include <iostream>
#include <vector>
#include <stdbool.h>


int main() {
int n;
std::cout << "¿Cuántos números? ";
std::cin >> n;
std::vector<int> numeros(n);
std::cout << "Ingresa los números:" << std::endl;
for(int i=0; i<n; ++i) std::cin >> numeros[i];


bool ordenado = true;
for (int i = 0; i < n - 1; ++i) {
if (numeros[i] >= numeros[i + 1]) {
ordenado = false;
break;
}
}


if (ordenado) {
std::cout << "El array está ordenado de forma ascendente." << std::endl;
} else {
std::cout << "El array NO está ordenado de forma ascendente." << std::endl;
}


return 0;
}
