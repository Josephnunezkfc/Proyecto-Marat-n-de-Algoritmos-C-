#include <iostream>


int main() {
int mat[2][2], transpuesta[2][2];


// ... (código para leer la matriz 'mat')


// Transponer la matriz
for (int i = 0; i < 2; ++i) {
for (int j = 0; j < 2; ++j) {
transpuesta[j][i] = mat[i][j];
}
}


// ... (código para mostrar la matriz 'transpuesta')
return 0;
}
