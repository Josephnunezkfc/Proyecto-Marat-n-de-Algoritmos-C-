#include <iostream>
#include <vector>


int main() {
int n;
// 1. Pedir al usuario el tamaño del vector y sus elementos
std::cout << "Ingresa el tamaño del vector: ";
std::cin >> n;
if (n <= 0) return 1;
std::vector<int> numeros(n);
std::cout << "Ingresa los " << n << " elementos del vector:" << std::endl;
for (int i = 0; i < n; ++i) {
std::cin >> numeros[i];
}
// 2. Inicializar variables
int elementoMasFrecuente = numeros[0];
int maxFrecuencia = 1;
// 3. Bucles anidados para encontrar el más frecuente
for (int i = 0; i < n; ++i) {
int frecuenciaActual = 0;
// Bucle interior para contar la frecuencia de numeros[i]
for (int j = 0; j < n; ++j) {
if (numeros[i] == numeros[j]) {
frecuenciaActual++;
}
}
// 4. Comparar con la frecuencia máxima encontrada hasta ahora
if (frecuenciaActual > maxFrecuencia) {
// 5. Actualizar si se encuentra una nueva frecuencia máxima
maxFrecuencia = frecuenciaActual;
elementoMasFrecuente = numeros[i];
}
}
// 6. Mostrar el resultado
std::cout << "El elemento más frecuente es " << elementoMasFrecuente
<< " que aparece " << maxFrecuencia << " veces." << std::endl;
return 0;
}
